#include <stdio.h>
#include <cstdlib>
#include <time.h>
#include <iostream>

using namespace std;


int main(int argc, char** argv) 
	{
	int i, n, s;
  s:=0;
  int array[100];
  for (i=0, i<100, i++)
    { if (array[i]<0)
        s=s+array[i]*(-1);
      else
        s=s+array[i];
     }
   cout << "Sum = " << s;
   }
